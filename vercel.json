{
  "version": 2,
  "headers": [
    {
      "source": "/(.*)\\.(png|jpg|jpeg|webp|avif|svg|ico|css|js|woff2?)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)\\.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/",
      "dest": "/join-waitlist.html"
    },
    {
      "src": "/join-waitlist",
      "dest": "/join-waitlist.html"
    },
    {
      "src": "/waitlist",
      "dest": "/waitlist.html"
    },
    {
      "src": "/index",
      "dest": "/index.html"
    }
  ]
}
