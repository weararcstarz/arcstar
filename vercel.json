{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node-build"
    },
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.png",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.css",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/",
      "dest": "/join-waitlist.html"
    },
    {
      "src": "/join-waitlist",
      "dest": "/join-waitlist.html"
    },
    {
      "src": "/waitlist",
      "dest": "/waitlist.html"
    },
    {
      "src": "/index",
      "dest": "/index.html"
    }
  ]
}